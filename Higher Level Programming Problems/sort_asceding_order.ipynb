   "cell_type": "code",
   "execution_count": 4,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "[-7, 1, 1, 2, 2, 3, 4, 5, 6]\n"
     ]
    }
   ],
   "source": [
    "lst1=[2,2,3,4,1,-7,5,6,1] #taken a list randomly\n",
    "for i in range(len(lst1)):\n",
    "    for j in range(i+1,len(lst1)):\n",
    "        if lst1[i]>lst1[j]:\n",
    "            lst1[i],lst1[j]=lst1[j],lst1[i] #swaping the two elements\n",
    "print(lst1)\n",
    "    "
   ]
  }
