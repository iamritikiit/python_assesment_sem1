   "source": [
    "Q.Given a positive integer, find out its factorial"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "metadata": {},
   "outputs": [],
   "source": [
    "def find_fact(n):\n",
    "    if n>1:\n",
    "        return n*find_fact(n-1)\n",
    "    elif n==1 or n==0:\n",
    "        return 1\n",
    "    "
   ]
